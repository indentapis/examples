{
  "private": "true",
  "name": "terraform-gcloud-google-groups-webhook",
  "version": "1.0.0",
  "description": "A Node.js starter for Terraform on GCP with Indent and Google Groups",
  "scripts": {
    "bundle": "ncc build lib/index.js -m -o dist",
    "build": "tsc",
    "build:env": "yaml2json .env.yaml > .env.json",
    "deploy": "npm run build && npm run bundle && cd terraform && terraform apply"
  },
  "main": "index.js",
  "repository": "https://github.com/indentapis/indent-js.git",
  "author": "Indent Inc",
  "license": "Apache-2.0",
  "dependencies": {
    "@google-cloud/storage": "^5.1.1",
    "@indent/types": "^0.1.3",
    "@indent/webhook": "^0.1.5",
    "@types/express": "^4.17.7",
    "@types/node": "^14.0.27",
    "@types/node-fetch": "^2.5.5",
    "axios": "^0.21.1",
    "gaxios": "^4.0.1",
    "googleapis": "^65.0.0"
  },
  "devDependencies": {
    "@google-cloud/functions-framework": "^1.6.0",
    "@vercel/ncc": "^0.29.0",
    "@zeit/ncc": "^0.22.3",
    "env-cmd": "^10.1.0",
    "typescript": "^4.3.5",
    "yamljs": "^0.3.0"
  }
}
